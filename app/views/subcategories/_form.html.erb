<%= simple_form_for(@subcategory, html: {class: "form-horizontal" }) do |f| %>
  <%= f.error_notification %>


      <div class="form-inputs">
            <%= f.input :description %>
            <%= f.input :category %>
       </div>


  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
